---
import BaseLayout from "../layouts/BaseLayout.astro";
import { Image } from "astro:assets";
import myimage from "../assets/images/teamus.png";
---

<BaseLayout>
  <section
    class="dark:text-zinc-50 text-stone-900 grid md:grid-cols-2 items-center justify-center gap-x-6"
  >
    <div class="w-full text-center md:text-left">
      <h1
        class="font-bold text-6xl items-center my-6 text-transparent bg-clip-text bg-gradient-to-r from-orange-400 to-rose-600"
      >
        Hi <span class="animate-wave inline-block text-black">&#128075;</span> !
      </h1>
      <h2 class="text-4xl text-inherit my-4 font-bold">
        I am <span
          class="text-transparent bg-clip-text bg-gradient-to-r from-teal-500 to-sky-600"
          >Aritra Sen</span
        >
      </h2>
      <h3
        class="text-transparent bg-clip-text bg-gradient-to-r from-indigo-500 to-rose-500"
      >
        An Ambitious Tech Adventurer and Problem Solver
      </h3>
    </div>
    <Image src={myimage} alt="Zig code" />
  </section>

  <section class="text-center bg-stone-200 dark:bg-stone-800 rounded-3xl py-6">
    <h1 class="m-0 p-0 py-6 text-2xl md:text-4xl text-orange-600">
      Skills and Interest
    </h1>
    <div class="flex text-2xl md:text-4xl justify-center p-6">
      I <span class="animate-bounce">&#10084;</span>
    </div>
    <div
      class="flex text-3xl sm:text-5xl gap-x-12 gap-y-8 p-6 items-center justify-center flex-wrap"
